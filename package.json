    "@storybook/node-logger": "^6.1.1",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "postcss-cli": "^7.1.2",
    "react-is": "^17.0.1"
